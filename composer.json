{
    "name": "laminas/laminas-di",
    "description": " ",
    "license": "BSD-3-Clause",
    "keywords": [
        "laminas",
        "di"
    ],
    "homepage": "https://laminas.dev",
    "support": {
        "docs": "https://docs.laminas.dev/laminas-di/",
        "issues": "https://github.com/laminas/laminas-di/issues",
        "source": "https://github.com/laminas/laminas-di",
        "rss": "https://github.com/laminas/laminas-di/releases.atom",
        "chat": "https://laminas.dev/chat",
        "forum": "https://discourse.laminas.dev"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.3-dev",
            "dev-develop": "2.4-dev"
        }
    },
    "require": {
        "php": ">=5.3.23",
        "laminas/laminas-code": "self.version",
        "laminas/laminas-stdlib": "self.version",
        "laminas/laminas-zendframework-bridge": "^0.4.5 || ^1.0"
    },
    "require-dev": {
        "fabpot/php-cs-fixer": "1.7.*",
        "laminas/laminas-servicemanager": "self.version",
        "phpunit/phpunit": "~4.0",
        "satooshi/php-coveralls": "dev-master"
    },
    "suggest": {
        "laminas/laminas-servicemanager": "Laminas\\ServiceManager component"
    },
    "autoload": {
        "psr-4": {
            "Laminas\\Di\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LaminasTest\\Di\\": "test/"
        }
    },
    "replace": {
        "zendframework/zend-di": "self.version"
    }
}
