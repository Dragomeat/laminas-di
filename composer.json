{
    "name": "laminas/laminas-di",
    "description": "Automated dependency injection and instance manager",
    "license": "BSD-3-Clause",
    "keywords": [
        "laminas",
        "di",
        "psr-11"
    ],
    "homepage": "https://laminas.dev",
    "support": {
        "docs": "https://docs.laminas.dev/laminas-di/",
        "issues": "https://github.com/laminas/laminas-di/issues",
        "source": "https://github.com/laminas/laminas-di",
        "rss": "https://github.com/laminas/laminas-di/releases.atom",
        "chat": "https://laminas.dev/chat",
        "forum": "https://discourse.laminas.dev"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.0-dev",
            "dev-develop": "3.1-dev"
        },
        "laminas": {
            "component": "Laminas\\Di",
            "config-provider": "Laminas\\Di\\ConfigProvider"
        }
    },
    "require": {
        "php": "^7.1",
        "laminas/laminas-stdlib": "^2.7 || ^3.0",
        "laminas/laminas-zendframework-bridge": "^0.4.5 || ^1.0",
        "psr/container": "^1.0"
    },
    "require-dev": {
        "container-interop/container-interop": "^1.2.0",
        "laminas/laminas-code": "^2.6 || ^3.0",
        "laminas/laminas-coding-standard": "~1.0.0",
        "laminas/laminas-servicemanager": "^3.0",
        "mikey179/vfsstream": "^1.6",
        "phpunit/phpunit": "^6.3"
    },
    "conflict": {
        "laminas/laminas-servicemanager-di": "*"
    },
    "suggest": {
        "laminas/laminas-code": "Required if you want to generate code",
        "laminas/laminas-servicemanager": "An IoC container without auto wiring capabilities"
    },
    "autoload": {
        "psr-4": {
            "Laminas\\Di\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LaminasTest\\Di\\": "test/"
        }
    },
    "scripts": {
        "check": [
            "@cs-check",
            "@test"
        ],
        "cs-check": "phpcs",
        "cs-fix": "phpcbf",
        "test": "phpunit --colors=always",
        "test-coverage": "phpunit --colors=always --coverage-clover clover.xml",
        "upload-coverage": "coveralls -v"
    },
    "replace": {
        "zendframework/zend-di": "self.version"
    }
}
